"data/blueprints.xml.append$//weaponBlueprint[@name=\"OG_TURRET_MISSILE_DAWN\"]/title"
msgstr "Двойная ракетная турель Рассвета"

#: src-en/data/blueprints.xml.append:1351
msgid ""
"data/blueprints.xml.append$//weaponBlueprint[@name=\"OG_TURRET_MISSILE_DAWN\"]/short"
